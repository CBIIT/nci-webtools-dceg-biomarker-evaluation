.input
  overflow auto
  margin 0px auto
  width 100%
  max-width 600px
  .table_data
    &:first-child
      min-width 11em
    &:nth-child(2)
      min-width 7em
    &:nth-child(3)
      float right
    &:last-child
      min-width 7em
.output
  @media (max-width 379px)
    .ui-tabs .ui-tabs-panel
      padding 1em 0px
  .table-side
    overflow auto
    .table_padding
      background-color #F8FAFA
      font-weight bold
      text-align center
